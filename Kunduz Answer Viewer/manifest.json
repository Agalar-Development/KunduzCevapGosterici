{
    "manifest_version": 3,
    "name": "Kunduz Answer Viewer",
    "description": "Shows the answer when the specified link is clicked.",
    "version": "0.1",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "content_scripts": [
      {
        "js": [
          "content-script.js"
        ],
        "matches": [
          "https://kunduz.com/*"
        ]
      }
    ]
  }